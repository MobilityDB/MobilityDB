"-- \"STBOX ZT((1,1,1,2011-12-31),(2,2,2,2012-01-04))\""
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:218
#, no-c-format
msgid "setPrecision"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:219
#, no-c-format
msgid ""
"Round the value or the coordinates of the bounding box to a number of "
"decimal places"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:220
#, no-c-format
msgid "setPrecision(box,integer): box"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:221
#, no-c-format
msgid ""
"SELECT setPrecision(tbox 'TBOX((1.12345, 2000-01-01), (2.12345, "
"2000-01-02))', 2);\n"
"-- \"TBOX((1.12,2000-01-01),(2.12,2000-01-02))\"\n"
"SELECT setPrecision(stbox 'STBOX T((1.12345, 1.12345, 2000-01-01),\n"
"        (2.12345, 2.12345, 2000-01-02))', 2);\n"
"-- \"STBOX T((1.12,1.12,2000-01-01),(2.12,2.12,2000-01-02))\""
msgstr ""

#. Tag: title
#: manipulating_box_types.xml:228
#, no-c-format
msgid "Spatial Reference System Functions"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:232
#, no-c-format
msgid "SRID"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:233
#, no-c-format
msgid "Get the spatial reference identifier &Z_support; &geography_support;"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:234
#, no-c-format
msgid "SRID(stbox): integer"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:235
#, no-c-format
msgid ""
"SELECT SRID(stbox 'STBOX ZT((1.0, 2.0, 3.0, 2000-01-01), (4.0, 5.0, 6.0, "
"2000-01-02))');\n"
"-- 0\n"
"SELECT SRID(stbox 'SRID=5676;STBOX T((1.0, 2.0, 2000-01-01), (4.0, 5.0, "
"2000-01-02))');\n"
"-- 5676\n"
"SELECT SRID(geodstbox 'GEODSTBOX T((, , 2000-01-01), (, , 2000-01-02))');\n"
"-- 4326"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:239
#, no-c-format
msgid "setSRID"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:240
#, no-c-format
msgid "Set the spatial reference identifier &Z_support; &geography_support;"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:241
#, no-c-format
msgid "setSRID(stbox): stbox"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:242
#, no-c-format
msgid ""
"SELECT setSRID(stbox 'STBOX ZT((1.0, 2.0, 3.0, 2000-01-01),\n"
"  (4.0, 5.0, 6.0, 2000-01-02))', 5676);\n"
"-- \"SRID=5676;STBOX ZT((1,2,3,2000-01-01),(4,5,6,2000-01-02))\""
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:246
#, no-c-format
msgid "transform"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:247
#, no-c-format
msgid "Transform to a different spatial reference"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:248
#, no-c-format
msgid "transform(stbox,integer): stbox &Z_support; &geography_support;"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:249
#, no-c-format
msgid ""
"SELECT transform(stbox 'SRID=4326;STBOX T((2.340088, 49.400250, "
"2000-01-01),\n"
"  (6.575317, 51.553167, 2000-01-02))', 3812);\n"
"-- \"SRID=3812;STBOX T((502773.429980817,511805.120401577,2000-01-01),\n"
"  (803028.908264815,751590.742628986,2000-01-02))\""
msgstr ""

#. Tag: title
#: manipulating_box_types.xml:255
#, no-c-format
msgid "Aggregate Functions"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:259
#, no-c-format
msgid "extent"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:260
#, no-c-format
msgid "Bounding box extent"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:261
#, no-c-format
msgid "extent(box): box"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:262
#, no-c-format
msgid ""
"WITH boxes(b) AS (\n"
"  SELECT tbox 'TBOX((1, 2000-01-01),(3, 2000-01-03))' UNION\n"
"  SELECT tbox 'TBOX((5, 2000-01-05),(7, 2000-01-07))' UNION\n"
"  SELECT tbox 'TBOX((6, 2000-01-06),(8, 2000-01-08))'\n"
")\n"
"SELECT extent(b) FROM boxes;\n"
"-- TBOX((1,2000-01-01 00:00:00+01),(8,2000-01-08 00:00:00+01))\n"
"WITH boxes(b) AS (\n"
"  SELECT stbox 'STBOX Z((1, 1, 1),(3, 3, 3))' UNION\n"
"  SELECT stbox 'STBOX Z((5, 5, 5),(7, 7, 7))' UNION\n"
"  SELECT stbox 'STBOX Z((6, 6, 6),(8, 8, 8))'\n"
")\n"
"SELECT extent(b) FROM boxes;\n"
"-- STBOX Z((1,1,1),(8,8,8))"
msgstr ""

#. Tag: title
#: manipulating_box_types.xml:268
#, no-c-format
msgid "Comparison Operators"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:270
#, no-c-format
msgid ""
"The traditional comparison operators (<varname>=</varname>, <varname>&lt;</"
"varname>, and so on) can be applied to box types. Excepted equality and "
"inequality, the other comparison operators are not useful in the real world "
"but allow B-tree indexes to be constructed on box types. These operators "
"compare first the timestamps and if those are equal, compare the values."
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:274
#, no-c-format
msgid "<varname>=</varname>"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:275
#, no-c-format
msgid "Are the bounding boxes equal?"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:276
#, no-c-format
msgid "box = box: boolean"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:277
#, no-c-format
msgid ""
"SELECT tbox 'TBOX((1, 2012-01-01), (1, 2012-01-04))' =\n"
"  tbox 'TBOX((2, 2012-01-03), (2, 2012-01-05))';\n"
"-- false"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:281
#, no-c-format
msgid "&lt;&gt;"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:282
#, no-c-format
msgid "Are the bounding boxes different?"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:283
#, no-c-format
msgid "box &lt;&gt; box: boolean"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:284
#, no-c-format
msgid ""
"SELECT tbox 'TBOX((1, 2012-01-01), (1, 2012-01-04))' &lt;&gt;\n"
"  tbox 'TBOX((2, 2012-01-03), (2, 2012-01-05))'\n"
"-- true"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:288
#, no-c-format
msgid "&lt;"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:289
#, no-c-format
msgid "Is the first bounding box less than the second one?"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:290
#, no-c-format
msgid "box &lt; box: boolean"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:291
#, no-c-format
msgid ""
"SELECT tbox 'TBOX((1, 2012-01-01), (1, 2012-01-04))' &lt;\n"
"  tbox 'TBOX((1, 2012-01-03), (2, 2012-01-05))'\n"
"-- true"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:295
#, no-c-format
msgid "&gt;"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:296
#, no-c-format
msgid "Is the first bounding box greater than the second one?"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:297
#, no-c-format
msgid "box &gt; box: boolean"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:298
#, no-c-format
msgid ""
"SELECT tbox 'TBOX((1, 2012-01-03), (1, 2012-01-04))' &gt;\n"
"  tbox 'TBOX((1, 2012-01-01), (2, 2012-01-05))'\n"
"-- true"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:302
#, no-c-format
msgid "&lt;="
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:303
#, no-c-format
msgid "Is the first bounding box less than or equal to the second one?"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:304
#, no-c-format
msgid "box &lt;= box: boolean"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:305
#, no-c-format
msgid ""
"SELECT tbox 'TBOX((1, 2012-01-01), (1, 2012-01-04))' &lt;=\n"
"  tbox 'TBOX((2, 2012-01-03), (2, 2012-01-05))'\n"
"-- true"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:309
#, no-c-format
msgid "&gt;="
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:310
#, no-c-format
msgid "Is the first bounding box greater than or equal to the second one?"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:311
#, no-c-format
msgid "box &gt;= box: boolean"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:312
#, no-c-format
msgid ""
"SELECT tbox 'TBOX((1, 2012-01-01), (1, 2012-01-04))' &gt;=\n"
"  tbox 'TBOX((2, 2012-01-03), (2, 2012-01-05))'\n"
"-- false"
msgstr ""

#. Tag: title
#: manipulating_box_types.xml:318
#, no-c-format
msgid "Set Operators"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:320
#, no-c-format
msgid ""
"The set operators for box types are union (<varname>+</varname>) and "
"intersection (<varname>*</varname>). In the case of union, the operands must "
"have exactly the same dimensions, otherwise an error is raised. Furthermore, "
"if the operands do not overlap on all the dimensions and error is raised, "
"since in this would result in a box with disjoint values, which cannot be "
"represented. The operator computes the union on all dimensions that are "
"present in both arguments. In the case of intersection, the operands must "
"have at least one common dimension, otherwise an error is raised. The "
"operator computes the intersection on all dimensions that are present in "
"both arguments."
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:324
#, no-c-format
msgid "<varname>+</varname>"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:325
#, no-c-format
msgid "Union of the bounding boxes"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:326
#, no-c-format
msgid "box + box: box"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:327
#, no-c-format
msgid ""
"SELECT tbox 'TBOX((1,2001-01-01),(3,2001-01-03))' +\n"
"  tbox 'TBOX((2,2001-01-02),(4,2001-01-04))';\n"
"-- \"TBOX((1,2001-01-01),(4,2001-01-04))\"\n"
"SELECT stbox 'STBOX ZT((1,1,1,2001-01-01),(2,2,2,2001-01-02))' +\n"
"  stbox 'STBOX T((2,2,2001-01-01),(3,3,2001-01-03))';\n"
"-- ERROR:  Boxes must be of the same dimensionality\n"
"SELECT tbox 'TBOX((1,2001-01-01),(3,2001-01-02))' +\n"
"  tbox 'TBOX((2,2001-01-03),(4,2001-01-04))';\n"
"-- ERROR:  Result of box union would not be contiguous"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:331
#, no-c-format
msgid "<varname>*</varname>"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:332
#, no-c-format
msgid "Intersection of the bounding boxes"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:333
#, no-c-format
msgid "box * box: box"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:334
#, no-c-format
msgid ""
"SELECT tbox 'TBOX((1,2001-01-01),(3,2001-01-03))' *\n"
"  tbox 'TBOX((,2001-01-02),(,2001-01-04))';\n"
"-- \"TBOX((,2001-01-02),(,2001-01-03))\"\n"
"SELECT stbox 'STBOX ZT((1,1,1,2001-01-01),(3,3,3,2001-01-02))' *\n"
"  stbox 'STBOX((2,2),(4,4))';\n"
"-- \"STBOX((2,2),(3,3))\""
msgstr ""

#. Tag: title
#: manipulating_box_types.xml:340
#, no-c-format
msgid "Topological Operators"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:342
#, no-c-format
msgid ""
"There are five topological operators: overlaps (<varname>&amp;&amp;</"
"varname>), contains (<varname>@&gt;</varname>), contained (<varname>&lt;@</"
"varname>), same (<varname>~=</varname>), and adjacent (<varname>-|-</"
"varname>). The operators verify the topological relationship between the "
"bounding boxes taking into account the value and/or the time dimension for "
"as many dimensions that are present on both arguments."
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:344
#, no-c-format
msgid "The topological operators for bounding boxes are given next."
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:348
#, no-c-format
msgid "&amp;&amp;"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:349
#, no-c-format
msgid "Do the bounding boxes overlap?"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:350
#, no-c-format
msgid "box &amp;&amp; box: boolean"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:351
#, no-c-format
msgid ""
"SELECT tbox 'TBOX((1,2001-01-01),(3,2001-01-03))' &amp;&amp;\n"
"  tbox 'TBOX((2,2001-01-02),(4,2001-01-04))';\n"
"-- true\n"
"SELECT stbox 'STBOX T((1,1,2001-01-01),(2,2,2001-01-02))'&amp;&amp;\n"
"  stbox 'STBOX T((,2001-01-02),(,2001-01-02))';\n"
"-- true"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:355
#, no-c-format
msgid "@&gt;"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:356
#, no-c-format
msgid "Does the first bounding box contain the second one?"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:357
#, no-c-format
msgid "box @&gt; box: boolean"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:358
#, no-c-format
msgid ""
"SELECT tbox 'TBOX((1,2001-01-01),(4,2001-01-04))' @&gt;\n"
"  tbox 'TBOX((2,2001-01-01),(3,2001-01-02))';\n"
"-- true\n"
"SELECT stbox 'STBOX Z((1,1,1),(3,3,3))' @&gt;\n"
"  stbox 'STBOX T((1,1,2001-01-01),(2,2,2001-01-02))';\n"
"-- true"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:362
#, no-c-format
msgid "&lt;@"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:363
#, no-c-format
msgid "Is the first bounding box contained in the second one?"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:364
#, no-c-format
msgid "box &lt;@ box: boolean"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:365
#, no-c-format
msgid ""
"SELECT tbox 'TBOX((1,2001-01-01),(2,2001-01-02))' &lt;@\n"
"  tbox 'TBOX((1,2001-01-01),(2,2001-01-02))';\n"
"-- true\n"
"SELECT stbox 'STBOX T((1,1,2001-01-01),(2,2,2001-01-02))' &lt;@\n"
"  stbox 'STBOX ZT((1,1,1,2001-01-01),(2,2,2,2001-01-02))';\n"
"-- true"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:369
#, no-c-format
msgid "<varname>~=</varname>"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:370
#, no-c-format
msgid "Are the bounding boxes equal in their common dimensions?"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:371
#, no-c-format
msgid "box ~= box: boolean"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:372
#, no-c-format
msgid ""
"SELECT tbox 'TBOX((1,2001-01-01),(2,2001-01-02))' ~=\n"
"  tbox 'TBOX((,2001-01-01),(,2001-01-02))';\n"
"-- true\n"
"SELECT stbox 'STBOX T((1,1,2001-01-01),(3,3,2001-01-03))' ~=\n"
"  stbox 'STBOX Z((1,1,1),(3,3,3))';\n"
"-- true"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:376
#, no-c-format
msgid "<varname>-|-</varname>"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:377
#, no-c-format
msgid "Are the bounding boxes adjacent?"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:378
#, no-c-format
msgid "box -|- box: boolean"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:379
#, no-c-format
msgid ""
"Two boxes are adjacent if they share <emphasis>n</emphasis> dimensions and "
"their intersection is at most of <emphasis>n</emphasis>-1 dimensions."
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:380
#, no-c-format
msgid ""
"SELECT tbox 'TBOX((1,2001-01-01),(2,2001-01-02))' -|-\n"
"  tbox 'TBOX((,2001-01-02),(,2001-01-03))';\n"
"-- true\n"
"SELECT stbox 'STBOX T((1,1,2001-01-01),(3,3,2001-01-03))' -|-\n"
"  stbox 'STBOX T((2,2,2001-01-03),(4,4,2001-01-04))';\n"
"-- true"
msgstr ""

#. Tag: title
#: manipulating_box_types.xml:386
#, no-c-format
msgid "Relative Position Operators"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:388
#, no-c-format
msgid ""
"These operators consider the relative position of the bounding boxes. The "
"operators <varname>&lt;&lt;</varname>, <varname>&gt;&gt;</varname>, "
"<varname>&amp;&lt;</varname>, and <varname>&amp;&gt;</varname> consider the "
"X value for the <varname>tbox</varname> type and the X coordinates for the "
"<varname>stbox</varname> type, the operators <varname>&lt;&lt;|</varname>, "
"<varname>|&gt;&gt;</varname>, <varname>&amp;&lt;|</varname>, and <varname>|"
"&amp;&gt;</varname> consider the Y coordinates for the <varname>stbox</"
"varname> type, the operators <varname>&lt;&lt;/</varname>, <varname>/&gt;&gt;"
"</varname>, <varname>&amp;&lt;/</varname>, and <varname>/&amp;&gt;</varname> "
"consider the Z coordinates for the <varname>stbox</varname> type, and the "
"operators <varname>&lt;&lt;#</varname>, <varname>#&gt;&gt;</varname>, "
"<varname>#&amp;&lt;</varname>, and <varname>#&amp;&gt;</varname> consider "
"the time dimension for the <varname>tbox</varname> and <varname>stbox</"
"varname> types. The operators raise an error if both boxes do not have the "
"required dimension."
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:390
#, no-c-format
msgid ""
"The operators for the numeric value dimension of the <varname>tbox</varname> "
"type are given next."
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:394 manipulating_box_types.xml:425
#, no-c-format
msgid "&lt;&lt;"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:395
#, no-c-format
msgid ""
"Are the X values of the first bounding box strictly less than those of the "
"second one?"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:396
#, no-c-format
msgid "tbox &lt;&lt; tbox: boolean"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:397
#, no-c-format
msgid ""
"SELECT tbox 'TBOX((1,2012-01-01),(2,2012-01-02))' &lt;&lt;\n"
"  tbox 'TBOX((3,2012-01-03),(4,2012-01-04))';\n"
"-- true\n"
"SELECT tbox 'TBOX((1,2012-01-01),(2,2012-01-02))' &lt;&lt;\n"
"  tbox 'TBOX((,2012-01-03),(,2012-01-04))';\n"
"-- ERROR:  Boxes must have X dimension"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:401 manipulating_box_types.xml:432
#, no-c-format
msgid "&gt;&gt;"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:402
#, no-c-format
msgid ""
"Are the X values of the first bounding box strictly greater than those of "
"the second one?"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:403
#, no-c-format
msgid "tbox &gt;&gt; tbox: boolean"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:404
#, no-c-format
msgid ""
"SELECT tbox 'TBOX((3,2012-01-03),(4,2012-01-04))' &gt;&gt;\n"
"  tbox 'TBOX((1,2012-01-01),(2,2012-01-02))';\n"
"-- true"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:408 manipulating_box_types.xml:439
#, no-c-format
msgid "&amp;&lt;"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:409
#, no-c-format
msgid ""
"Are the X values of the first bounding box not greater than those of the "
"second one?"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:410
#, no-c-format
msgid "tbox &amp;&lt; tbox: boolean"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:411
#, no-c-format
msgid ""
"SELECT tbox 'TBOX((1,2012-01-01),(4,2012-01-04))' &amp;&lt;\n"
"  tbox 'TBOX((3,2012-01-03),(4,2012-01-04))';\n"
"-- true"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:415 manipulating_box_types.xml:446
#, no-c-format
msgid "&amp;&gt;"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:416
#, no-c-format
msgid ""
"Are the X values of the first bounding box not less than those of the second "
"one?"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:417
#, no-c-format
msgid "tbox &amp;&gt; tbox: boolean"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:418
#, no-c-format
msgid ""
"SELECT tbox 'TBOX((1,2012-01-01),(2,2012-01-02))' &amp;&gt;\n"
"  tbox 'TBOX((1,2012-01-01),(4,2012-01-04))';\n"
"-- true"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:421
#, no-c-format
msgid ""
"The operators for the spatial value dimension of the <varname>stbox</"
"varname> type are given next."
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:426
#, no-c-format
msgid ""
"Are the X values of the first bounding box strictly to the left of those of "
"the second one?"
msgstr ""

#. Tag: varname
#: manipulating_box_types.xml:427
#, no-c-format
msgid "stbox &lt;&lt; stbox: boolean"
msgstr ""

#. Tag: programlisting
#: manipulating_box_types.xml:428
#, no-c-format
msgid ""
"SELECT stbox 'STBOX Z((1,1,1),(2,2,2))' &lt;&lt; stbox 'STBOX Z((3,3,3),"
"(4,4,4))';\n"
"-- true"
msgstr ""

#. Tag: para
#: manipulating_box_types.xml:433
#, no-c-format
msgid ""
"Are the X values of the first bounding box strictly to the right of those of "
"the second one?"
msgstr ""

#. Tag: varname
