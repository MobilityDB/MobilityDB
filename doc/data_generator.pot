msgstr ""

#. Tag: programlisting
#: data_generator.xml:323
#, no-c-format
msgid ""
"CREATE TABLE tbl_geom_point AS\n"
"SELECT 1 AS k, geometry 'SRID=3812;point empty' AS g UNION\n"
"SELECT k, random_geom_point(0, 100, 0, 100, 3812)\n"
"FROM generate_series(2, size) k;"
