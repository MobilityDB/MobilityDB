#. Tag: title
#: data_generator.xml:295
#, no-c-format
msgid "Generation of Tables with Random Values"
msgstr ""

#. Tag: para
#: data_generator.xml:297
#, no-c-format
msgid ""
"The files <ulink url=\"https://github.com/MobilityDB/MobilityDB/datagen/"
"general/create_test_tables_temporal.sql\">create_test_tables_temporal.sql</"
"ulink> and <ulink url=\"https://github.com/MobilityDB/MobilityDB/datagen/"
"point/create_test_tables_tpoint.sql\">create_test_tables_tpoint.sql</ulink> "
"provide usage examples for the functions generating random values listed "
"above. For example, the first file defines the following function."
msgstr ""

#. Tag: programlisting
#: data_generator.xml:304
#, no-c-format
msgid ""
"CREATE OR REPLACE FUNCTION create_test_tables_temporal(size int DEFAULT "
"100)\n"
"RETURNS text AS $$\n"
"DECLARE\n"
"  perc int;\n"
"BEGIN\n"
"perc := size * 0.01;\n"
"IF perc &lt; 1 THEN perc := 1; END IF;\n"
"\n"
"-- ... Table generation ...\n"
"\n"
"RETURN 'The End';\n"
"END;\n"
"$$ LANGUAGE 'plpgsql';"
msgstr ""

#. Tag: para
#: data_generator.xml:305
#, no-c-format
msgid ""
"The function has a <varname>size</varname> parameter that defines the number "
"of rows in the tables. If not provided, it creates by default tables of 100 "
"rows. The function defines a variable <varname>perc</varname> that computes "
"the 1% of the size of the tables. This parameter is used, for example, for "
"generating tables having 1% of null values. We illustrate next some of the "
"commands generating tables."
msgstr ""

#. Tag: para
#: data_generator.xml:307
#, no-c-format
msgid ""
"The creation of a table <varname>tbl_float</varname> containing random "
"<varname>float</varname> values in the range [0,100] with 1% of null values "
"is given next."
msgstr ""

#. Tag: programlisting
#: data_generator.xml:308
#, no-c-format
msgid ""
"CREATE TABLE tbl_float AS\n"
"/* Add perc NULL values */\n"
"SELECT k, NULL AS f\n"
"FROM generate_series(1, perc) AS k UNION\n"
"SELECT k, random_float(0, 100)\n"
"FROM generate_series(perc+1, size) AS k;"
msgstr ""

#. Tag: para
#: data_generator.xml:310
#, no-c-format
msgid ""
"The creation of a table <varname>tbl_tbox</varname> containing random "
"<varname>tbox</varname> values where the bounds for values are in the range "
"[0,100] and the bounds for timestamps are in the range [2001-01-01, "
"2001-12-31] is given next."
msgstr ""

#. Tag: programlisting
#: data_generator.xml:311
#, no-c-format
msgid ""
"CREATE TABLE tbl_tbox AS\n"
"/* Add perc NULL values */\n"
"SELECT k, NULL AS b\n"
"FROM generate_series(1, perc) AS k UNION\n"
"SELECT k, random_tbox(0, 100, '2001-01-01', '2001-12-31', 10, 10)\n"
"FROM generate_series(perc+1, size) AS k;"
msgstr ""

#. Tag: para
#: data_generator.xml:313
#, no-c-format
msgid ""
"The creation of a table <varname>tbl_floatrange</varname> containing random "
"<varname>floatrange</varname> values where the bounds for values are in the "
"range [0,100] and the maximum difference between the lower and the upper "
"bounds is 10 is given next."
msgstr ""

#. Tag: programlisting
#: data_generator.xml:314
#, no-c-format
msgid ""
"CREATE TABLE tbl_floatrange AS\n"
"/* Add perc NULL values */\n"
"SELECT k, NULL AS f\n"
"FROM generate_series(1, perc) AS k UNION\n"
"SELECT k, random_floatrange(0, 100, 10)\n"
"FROM generate_series(perc+1, size) AS k;"
msgstr ""

#. Tag: para
#: data_generator.xml:316
#, no-c-format
msgid ""
"The creation of a table <varname>tbl_timestampset</varname> containing "
"random <varname>timestampset</varname> values having between 5 and 10 "
"timestamps where the timestamps are in the range [2001-01-01, 2001-12-31] "
"and the maximum interval between consecutive timestamps is 10 minutes is "
"given next."
msgstr ""

#. Tag: programlisting
#: data_generator.xml:317
#, no-c-format
msgid ""
"CREATE TABLE tbl_timestampset AS\n"
"/* Add perc NULL values */\n"
"SELECT k, NULL AS ts\n"
"FROM generate_series(1, perc) AS k UNION\n"
"SELECT k, random_timestampset('2001-01-01', '2001-12-31', 10, 5, 10)\n"
"FROM generate_series(perc+1, size) AS k;"
msgstr ""

#. Tag: para
#: data_generator.xml:319
#, no-c-format
msgid ""
"The creation of a table <varname>tbl_period</varname> containing random "
"<varname>period</varname> values where the timestamps are in the range "
"[2001-01-01, 2001-12-31] and the maximum difference between the lower and "
