msgstr "Constructor para tipos temporales de subtipo secuencia"

#. Tag: varname
#: manipulating_temporal_types.xml:189
#, no-c-format
msgid "tdisc_seq(base,period): tdisc_seq"
msgstr "tdisc_seq(base,period): tdisc_seq"

