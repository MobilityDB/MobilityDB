"SELECT startSequence(tfloat '{[1@2012-01-01, 2@2012-01-03),\n"
"  [3@2012-01-03, 5@2012-01-05)}');\n"
"-- \"[1@2012-01-01, 2@2012-01-03)\""
msgstr ""
"SELECT startSequence(tfloat '{[1@2012-01-01, 2@2012-01-03),\n"
"  [3@2012-01-03, 5@2012-01-05)}');\n"
"-- \"[1@2012-01-01, 2@2012-01-03)\""

#. Tag: varname
#: manipulating_temporal_types.xml:544
#, no-c-format
msgid "endSequence"
msgstr "endSequence"

#. Tag: para
#: manipulating_temporal_types.xml:545
#, no-c-format
msgid "Get the end sequence"
msgstr "Obtener la secuencia final"

#. Tag: varname
#: manipulating_temporal_types.xml:546
#, no-c-format
msgid "endSequence({ttype_seq,ttype_seqset}): ttype_seq"
msgstr "endSequence({ttype_seq,ttype_seqset}): ttype_seq"

#. Tag: programlisting
#: manipulating_temporal_types.xml:547
#, no-c-format
msgid ""
"SELECT endSequence(tfloat '{[1@2012-01-01, 2@2012-01-03), [3@2012-01-03, "
"5@2012-01-05)}');\n"
"-- \"[3@2012-01-03, 5@2012-01-05)\""
msgstr ""
"SELECT endSequence(tfloat '{[1@2012-01-01, 2@2012-01-03), [3@2012-01-03, "
"5@2012-01-05)}');\n"
"-- \"[3@2012-01-03, 5@2012-01-05)\""

#. Tag: varname
#: manipulating_temporal_types.xml:551
#, no-c-format
msgid "sequenceN"
msgstr "sequenceN"

#. Tag: para
#: manipulating_temporal_types.xml:552
#, no-c-format
msgid "Get the n-th sequence"
msgstr "Obtener la enésima secuencia"

#. Tag: varname
#: manipulating_temporal_types.xml:553
#, no-c-format
msgid "sequenceN({ttype_seq,ttype_seqset},integer): ttype_seq"
msgstr "sequenceN({ttype_seq,ttype_seqset},integer): ttype_seq"

#. Tag: programlisting
#: manipulating_temporal_types.xml:554
#, no-c-format
msgid ""
"SELECT sequenceN(tfloat '{[1@2012-01-01, 2@2012-01-03),\n"
"  [3@2012-01-03, 5@2012-01-05)}', 2);\n"
"-- \"[3@2012-01-03, 5@2012-01-05)\""
msgstr ""
"SELECT sequenceN(tfloat '{[1@2012-01-01, 2@2012-01-03),\n"
"  [3@2012-01-03, 5@2012-01-05)}', 2);\n"
"-- \"[3@2012-01-03, 5@2012-01-05)\""

#. Tag: varname
#: manipulating_temporal_types.xml:558
#, no-c-format
msgid "sequences"
msgstr "sequences"

#. Tag: para
#: manipulating_temporal_types.xml:559
#, no-c-format
msgid "Get the sequences"
msgstr "Obtener las secuencias"

#. Tag: varname
#: manipulating_temporal_types.xml:560
#, no-c-format
msgid "sequences({ttype_seq,ttype_seqset}): ttype_seq[]"
msgstr "sequences({ttype_seq,ttype_seqset}): ttype_seq[]"

#. Tag: programlisting
#: manipulating_temporal_types.xml:561
#, no-c-format
msgid ""
"SELECT sequences(tfloat '{[1@2012-01-01, 2@2012-01-03), [3@2012-01-03, "
"5@2012-01-05)}');\n"
