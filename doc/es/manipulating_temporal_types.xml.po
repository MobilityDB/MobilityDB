msgid "appendInstant"
msgstr "appendInstant"

#. Tag: para
#: manipulating_temporal_types.xml:318
#, no-c-format
msgid "Append a temporal instant to a temporal value"
msgstr "Anexar un instante temporal a un valor temporal"

#. Tag: varname
#: manipulating_temporal_types.xml:319
