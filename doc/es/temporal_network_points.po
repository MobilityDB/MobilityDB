#. Tag: varname
#: temporal_network_points.xml:557
#, no-c-format
msgid "tcount"
msgstr "tcount"

#. Tag: para
#: temporal_network_points.xml:558
#, no-c-format
msgid "Temporal count"
msgstr "Conteo temporal"

#. Tag: varname
#: temporal_network_points.xml:559
#, no-c-format
msgid "tcount(tnpoint): {tint_instset,tint_seqset}"
msgstr "tcount(tnpoint): {tint_instset,tint_seqset}"

#. Tag: programlisting
#: temporal_network_points.xml:560
#, no-c-format
msgid ""
"WITH Temp(temp) AS (\n"
"SELECT tnpoint '[NPoint(1, 0.1)@2012-01-01, NPoint(1, 0.3)@2012-01-03)' "
"UNION\n"
"SELECT tnpoint '[NPoint(1, 0.2)@2012-01-02, NPoint(1, 0.4)@2012-01-04)' "
"UNION\n"
"SELECT tnpoint '[NPoint(1, 0.3)@2012-01-03, NPoint(1, 0.5)@2012-01-05)' )\n"
"SELECT tcount(Temp)\n"
"FROM Temp\n"
"-- {[1@2012-01-01, 2@2012-01-02, 1@2012-01-04, 1@2012-01-05)}"
msgstr ""
"WITH Temp(temp) AS (\n"
"SELECT tnpoint '[NPoint(1, 0.1)@2012-01-01, NPoint(1, 0.3)@2012-01-03)' "
"UNION\n"
"SELECT tnpoint '[NPoint(1, 0.2)@2012-01-02, NPoint(1, 0.4)@2012-01-04)' "
"UNION\n"
"SELECT tnpoint '[NPoint(1, 0.3)@2012-01-03, NPoint(1, 0.5)@2012-01-05)' )\n"
"SELECT tcount(Temp)\n"
"FROM Temp\n"
"-- {[1@2012-01-01, 2@2012-01-02, 1@2012-01-04, 1@2012-01-05)}"

