#. Tag: title
#: manipulating_box_types.xml:190
#, no-c-format
msgid "Modification Functions"
msgstr "Funciones de modificación"

#. Tag: para
#: manipulating_box_types.xml:192
#, no-c-format
msgid ""
"The functions given next expand the bounding boxes on the value and the time "
"dimension or set the precision of the value dimension. These functions raise "
"an error if the corresponding dimension is not present."
msgstr ""
"Las funciones dadas a continuación expanden los cuadros delimitadores en la "
"dimensión de valor y de tiempo o establecen la precisión de la dimensión de "
"valor. Estas funciones generan un error si la dimensión correspondiente no "
"está presente."

#. Tag: varname
#: manipulating_box_types.xml:197
#, no-c-format
msgid "expandValue"
msgstr "expandValue"

#. Tag: para
#: manipulating_box_types.xml:198
#, no-c-format
msgid "Expand the numeric value dimension of the bounding box by a float value"
msgstr ""
"Extender la dimensión de valor numérico del cuadro delimitador con un valor "
"flotante"

#. Tag: varname
#: manipulating_box_types.xml:199
#, no-c-format
msgid "expandValue(tbox,float): tbox"
msgstr "expandValue(tbox,float): tbox"

#. Tag: programlisting
#: manipulating_box_types.xml:200
#, no-c-format
msgid ""
"SELECT expandValue(tbox 'TBOX((1,2012-01-01),(2,2012-01-03))', 1);\n"
"-- \"TBOX((0,2012-01-01),(3,2012-01-03))\"\n"
"SELECT expandValue(tbox 'TBOX((,2012-01-01),(,2012-01-03))', 1);\n"
"-- The box must have value dimension"
msgstr ""
"SELECT expandValue(tbox 'TBOX((1,2012-01-01),(2,2012-01-03))', 1);\n"
"-- \"TBOX((0,2012-01-01),(3,2012-01-03))\"\n"
"SELECT expandValue(tbox 'TBOX((,2012-01-01),(,2012-01-03))', 1);\n"
"-- The box must have value dimension"

#. Tag: varname
#: manipulating_box_types.xml:204
#, no-c-format
msgid "expandSpatial"
msgstr "expandSpatial"

#. Tag: para
#: manipulating_box_types.xml:205
#, no-c-format
msgid "Expand the spatial value dimension of the bounding box by a float value"
msgstr ""
"Extender la dimensión de valor espacial del cuadro delimitador con un valor "
"flotante"

#. Tag: varname
#: manipulating_box_types.xml:206
#, no-c-format
msgid "expandSpatial(stbox,float): stbox"
msgstr "expandSpatial(stbox,float): stbox"

#. Tag: programlisting
#: manipulating_box_types.xml:207
#, no-c-format
msgid ""
"SELECT expandSpatial(stbox 'STBOX ZT((1,1,1,2012-01-01),"
"(2,2,2,2012-01-03))', 1);\n"
"-- \"STBOX ZT((0,0,0,2012-01-01),(3,3,3,2012-01-03))\"\n"
"SELECT expandSpatial(stbox 'STBOX T((,2012-01-01),(,2012-01-03))', 1);\n"
"-- The box must have XY dimension"
msgstr ""
"SELECT expandSpatial(stbox 'STBOX ZT((1,1,1,2012-01-01),"
"(2,2,2,2012-01-03))', 1);\n"
"-- \"STBOX ZT((0,0,0,2012-01-01),(3,3,3,2012-01-03))\"\n"
"SELECT expandSpatial(stbox 'STBOX T((,2012-01-01),(,2012-01-03))', 1);\n"
"-- The box must have XY dimension"

#. Tag: varname
#: manipulating_box_types.xml:211
#, no-c-format
msgid "expandTemporal"
msgstr "expandTemporal"

#. Tag: para
#: manipulating_box_types.xml:212
#, no-c-format
msgid "Expand the temporal dimension of the bounding box by a time interval"
msgstr ""
"Extender la dimensión temporal del cuadro delimitador con un intervalo de "
"tiempo"

#. Tag: varname
#: manipulating_box_types.xml:213
#, no-c-format
msgid "expandTemporal(box,interval): box"
msgstr "expandTemporal(box,interval): box"

#. Tag: programlisting
#: manipulating_box_types.xml:214
#, no-c-format
msgid ""
"SELECT expandTemporal(tbox 'TBOX((1,2012-01-01),(2,2012-01-03))', interval "
"'1 day');\n"
"-- \"TBOX((1,2011-12-31),(2,2012-01-04))\"\n"
"SELECT expandTemporal(stbox 'STBOX ZT((1,1,1,2012-01-01),"
"(2,2,2,2012-01-03))',\n"
"        interval '1 day');\n"
"-- \"STBOX ZT((1,1,1,2011-12-31),(2,2,2,2012-01-04))\""
msgstr ""
"SELECT expandTemporal(tbox 'TBOX((1,2012-01-01),(2,2012-01-03))', interval "
"'1 day');\n"
"-- \"TBOX((1,2011-12-31),(2,2012-01-04))\"\n"
"SELECT expandTemporal(stbox 'STBOX ZT((1,1,1,2012-01-01),"
"(2,2,2,2012-01-03))',\n"
"  interval '1 day');\n"
"-- \"STBOX ZT((1,1,1,2011-12-31),(2,2,2,2012-01-04))\""

#. Tag: varname
