"-- \"[2000-01-01, 2000-01-03]\"\n"
"SELECT stbox 'STBOX Z((1 1 1),(3 3 3))'::box2d;\n"
"-- \"BOX(1 1,3 3)\"\n"
"SELECT stbox 'STBOX Z((1 1 1),(3 3 3))'::box3d;\n"
