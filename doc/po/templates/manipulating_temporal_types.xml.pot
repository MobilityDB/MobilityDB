#. Tag: varname
#: manipulating_temporal_types.xml:295
#, no-c-format
msgid "interpolation(ttype): {'Discrete', 'Stepwise', 'Linear'}"
msgstr ""

#. Tag: programlisting
#: manipulating_temporal_types.xml:296
