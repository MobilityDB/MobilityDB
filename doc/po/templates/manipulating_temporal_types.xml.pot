#. Tag: varname
#: manipulating_temporal_types.xml:300
#, no-c-format
msgid "getValue"
msgstr ""

#. Tag: para
#: manipulating_temporal_types.xml:301
#, no-c-format
msgid "Get the value"
msgstr ""

#. Tag: varname
#: manipulating_temporal_types.xml:302
#, no-c-format
msgid "getValue(ttypeinst): base"
msgstr ""

#. Tag: programlisting
#: manipulating_temporal_types.xml:303
#, no-c-format
msgid ""
