				<listitem>
					<para><link linkend="time_timestamps"><varname>timestamps</varname></link>: Get the different timestamps</para>
				</listitem>

				<listitem>
					<para><link linkend="numPeriods"><varname>numPeriods</varname></link>: Get the number of periods</para>
				</listitem>

				<listitem>
