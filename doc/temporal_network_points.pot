#. Tag: programlisting
#: temporal_network_points.xml:197
#, no-c-format
msgid ""
"SELECT nsegment 'Nsegment(3, 0.5, 0.5)' &lt; nsegment 'Nsegment(3, 0.5, "
"0.6)';\n"
"-- true"
msgstr ""

#. Tag: varname
#: temporal_network_points.xml:201
#, no-c-format
msgid "&gt;"
msgstr ""

#. Tag: para
#: temporal_network_points.xml:202
#, no-c-format
msgid "Is the first value greater than the second one?"
msgstr ""

#. Tag: varname
#: temporal_network_points.xml:203
#, no-c-format
msgid "{npoint,nsegment} &gt; {npoint,nsegment}"
msgstr ""

#. Tag: programlisting
