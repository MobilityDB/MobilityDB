#: temporal_network_points.xml:189
#, no-c-format
msgid "{npoint,nsegment} &lt;&gt; {npoint,nsegment}"
msgstr ""

#. Tag: programlisting
#: temporal_network_points.xml:190
#, no-c-format
msgid ""
