msgid "wcount(tnpoint): {tint_instset,tint_seqset}"
msgstr ""

#. Tag: programlisting
#: temporal_network_points.xml:567
#, no-c-format
msgid ""
"WITH Temp(temp) AS (\n"
"SELECT tnpoint '[NPoint(1, 0.1)@2012-01-01, NPoint(1, 0.3)@2012-01-03)' "
"UNION\n"
"SELECT tnpoint '[NPoint(1, 0.2)@2012-01-02, NPoint(1, 0.4)@2012-01-04)' "
"UNION\n"
"SELECT tnpoint '[NPoint(1, 0.3)@2012-01-03, NPoint(1, 0.5)@2012-01-05)' )\n"
"SELECT wcount(Temp, '1 day')\n"
"FROM Temp\n"
"-- {[1@2012-01-01, 2@2012-01-02, 3@2012-01-03, 2@2012-01-04, 1@2012-01-05,\n"
"  1@2012-01-06)}"
msgstr ""

